<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yyhn.exam.mapper.AppPapersUserMapper">
    <update id="updatePapersByUserIdAndPapersId" parameterType="Integer">
        update exam_papers_user set is_papers=1 where user_id=#{userId} and papers_id=#{papersId}
    </update>
</mapper>
